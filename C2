}



_l4()
{
clear
while :
do
clear;clear
echo -en "

\033[1;91m╔══════════════════════════════════╗
\033[1;91m║${CYAN} [1] OVH                          \033[1;91m║
\033[1;91m║${CYAN} [2] NFO                          \033[1;91m║
\033[1;91m║${CYAN} [3] VECTOR                       \033[1;91m║
\033[1;91m║${CYAN} [4] HOME                         \033[1;91m║
\033[1;91m║${CYAN} [R] RETURN MAIN MENU             \033[1;91m║
\033[1;91m╚══════════════════════════════════╝
${CYAN}[\033[1;91mF u j i n${CYAN}]-家 "
    read yourch
    case $yourch in
      1) _ovh ;;
      2) _nfo ;;
      3) _vector ;;
      4) _home ;;
	  R) Take_input1 ;;
      *) echo "INVALID SELECTION," ;
         echo "PRESS ENTER." ; read ;;
 esac
done
}

trap exit_script 2



