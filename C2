Take_input1()
{
clear
while :
do
clear;clear
echo -en "
███████╗██╗   ██╗     ██╗██╗███╗   ██╗
██╔════╝██║   ██║     ██║██║████╗  ██║
█████╗  ██║   ██║     ██║██║██╔██╗ ██║
██╔══╝  ██║   ██║██   ██║██║██║╚██╗██║ ⿆⾐
██║     ╚██████╔╝╚█████╔╝██║██║ ╚████║
╚═╝      ╚═════╝  ╚════╝ ╚═╝╚═╝  ╚═══╝
              \033[1;91m罪 深 い Made By Ampx\033[00m

\033[1;91m╔══════════════════════════════════╗
\033[1;91m║${CYAN} [1] L4                           \033[1;91m║
\033[1;91m║${CYAN} [2] l7                           \033[1;91m║
\033[1;91m║${CYAN} [3] Tools                        \033[1;91m║
\033[1;91m║${CYAN} [4] Transfer to Remark C2        \033[1;91m║
\033[1;91m║${CYAN} [C] CREDITS.                     \033[1;91m║
\033[1;91m║${CYAN} [I] INFO.                        \033[1;91m║
\033[1;91m║${CYAN} [X] CLOSE.                       \033[1;91m║
\033[1;91m╚══════════════════════════════════╝
${CYAN}[\033[1;91mF u j i n${CYAN}]-家 "
    read yourch
    case $yourch in
	  1) _l4 ;;
	  2) _l7 ;;
    3) _tools
	  4) cortex  exit 1 ;;
	  5) snake ;;
	  6) speedtest ;;
	  7) vnstat -l -i eth0 ;;
	  8) dstat1 ;;
	  9) users ;;
	  10) last ; exit 1 ;;
	  C) credits ; exit 1 ;;
	  I) echo "cnc" ;;
	  X) exit 1  ;;
      *) echo "INVALID SELECTION," ;
         echo "PRESS ENTER." ; read ;;
 esac
done # end_while
}
